{
  "name": "@tianwenh/utils",
  "version": "0.2.1",
  "description": "List of util functions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tianwenh/utils.git"
  },
  "author": "tianwenh",
  "bugs": {
    "url": "https://github.com/tianwenh/utils/issues"
  },
  "homepage": "https://github.com/tianwenh/utils#readme",
